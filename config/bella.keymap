#define DEFAULT 0
#define LOWER   1
#define RAISE   2

#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&kp ESC     &kp N1  &kp N2  &kp N3     &kp N4     &kp N5    &kp F1       &kp F5    &kp N6    &kp N7     &kp N8     &kp N9   &kp N0    &kp DEL
&kp TAB     &kp Q   &kp W   &kp E      &kp R      &kp T     &kp F2       &kp F6    &kp Y     &kp U      &kp I      &kp O    &kp P     &kp BSLH
&kp CAPS    &kp A   &kp S   &kp D      &kp F      &kp G     &kp F3       &kp F7    &kp H     &kp J      &kp K      &kp L    &kp SEMI  &kp RET
&kp LSHIFT  &kp Z   &kp X   &kp C      &kp V      &kp B     &kp F4       &kp F8    &kp N     &kp M      &kp COMMA  &kp DOT  &kp FSLH  &kp RSHIFT
                            &kp LCTRL  &mo LOWER  &kp LALT  &kp SPACE    &kp BSPC  &kp RALT  &mo RAISE  &kp RCTRL
            >;
        };

        nav_layer {
            bindings = <
&trans  &trans    &trans    &trans     &trans  &trans  &kp F9     &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans    &kp UP    &trans     &trans  &trans  &kp F10    &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &kp LEFT  &kp DOWN  &kp RIGHT  &trans  &trans  &kp F11    &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans    &trans    &trans     &trans  &trans  &kp F12    &trans  &trans  &trans  &trans  &trans  &trans  &trans
                            &trans     &trans  &trans  &trans     &trans  &trans  &trans  &trans
            >;
        };

        symbol_layer {
            bindings = <
&sys_reset  &trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans  &bootloader
&trans      &trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans      &trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans  &trans
&trans      &trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans  &trans
                            &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans
            >;
        };
    };
};
